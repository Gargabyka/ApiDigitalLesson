{
  "Serilog": {
    "Using":  [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      { "Name": "Console" },
      { "Name": "File", "Args": { "path": "./logs/ApiDigitalLessonLogs.log" } }
    ]
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "{ConnectionString}"
  },
  "JWTSettings": {
    "Key": "{Key}",
    "Issuer": "{Issuer}",
    "Audience": "{Audience}",
    "DurationInMinutes": 130
  },
  "MailSettings": {
    "EmailFrom": "{EmailFrom}",
    "Sender": "{Sender}",
    "SmtpHost": "{SmtpHost}",
    "SmtpPort": 0,
    "SmtpUser": "{SmtpUser}",
    "SmtpPass": "{SmtpPass}",
    "DisplayName": "{DisplayName}"
  },
  "TelegramSettings": {
    "Token": "{Token}"
  },
  "GoogleAuthentication": {
    "ClientId": "{ClientId}",
    "ClientSecret": "{ClientSecret}"
  },   
  "VkAuthentication": {
    "ClientId": "{ClientId}",
    "ClientSecret": "{ClientSecret}"
  } 
}
